{
  "include": ["web/wp-content/themes/<%= slug %>/src/js/**/*.js"],
  "compilerOptions": {
    "target": "es2015",
    "module": "esnext",
    "noEmit": true,
    "checkJs": true,
    "strict": false,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "moduleResolution": "node"
  }
}
